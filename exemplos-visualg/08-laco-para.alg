algoritmo "semnome"

var
i : inteiro
senha : caractere
inicio

para i de 10 ate 0 passo -1 faca
   escreva("Digite a senha: ")
   leia(senha)
   limpatela
   escolha senha
   caso "qua209.003","java","senai"
      escreva("Senha Ok.")
      interrompa
   outrocaso
      escreval("Tentativas restantes ",i)
   fimescolha
fimpara

fimalgoritmo
