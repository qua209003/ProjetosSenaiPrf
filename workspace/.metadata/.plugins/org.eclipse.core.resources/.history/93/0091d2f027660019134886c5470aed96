
public class ExemploReferencia {

	public static void main(String[] args) {
		// ler página 55 da apostila
		Conta c1 = new Conta();
		c1.depositar(100);
		
		Conta c2 = c1;
		
		System.out.println(c1);
		System.out.println(c1.getSaldo());
		
		System.out.println(c2);
		System.out.println(c2.getSaldo());

	}

}
