
public class GerenciarConta {

	public static void main(String[] args) {

		Conta conta01 = new Conta();
		conta01.numero = 301;
		conta01.depositar(2000);
		conta01.nome = "Luiz Felipe";
		
		System.out.println(
				"Nome: "+ conta01.nome+
				"Saldo: "+ conta01.saldo				
				);

	}

}
